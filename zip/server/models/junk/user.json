{
  "name": "user",
  "plural": "user",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "username": {
      "type": "string",
      "required": false
    },
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "password": {
      "type": "string",
      "required": true
    },
    "created_at": {
      "type": "date",
      "required": true
    },
    "updated_at": {
      "type": "date",
      "required": true
    }
  },
  "validations": [],
  "relations": {

    "menuList": {
      "type": "referencesMany",
      "model": "Menu",
      "foreignKey": "menus",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "groceries": {
      "type": "referencesMany",
      "model": "Grocery",
      "foreignKey": "groceryIds",
      "options": {
        "validate": true,
        "forceId": false
      }
    }

  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
